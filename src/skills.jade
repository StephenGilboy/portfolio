section#skills(class="section section-bg-light section-top-fall section-bottom-fall")
  .section-inner
    .container
      .row
        div(class="col span_8")
          h3 My Skills
          div(class="separator l")
          p Here is a description of my skill set
        div(class="col span_4")
          .skills
            div(data-progress="90" class="skills-item wow")
              .skills-item-progress
              h4 C#
            div(data-progress="85" class="skills-item wow")
              .skills-item-progress
              h4 JavaScript
            div(data-progress="80" class="skills-item wow")
              .skills-item-progress
              h4 SQL
            div(data-progress="95" class="skills-item wow")
              .skills-item-progress
              h4 ASP.NET MVC/Web API
            div(data-progress="40" class="skills-item wow")
              .skills-item-progress
              h4 Blues Guitar